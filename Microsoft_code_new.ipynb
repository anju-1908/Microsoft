    "import pandas as pd\n",
    "import warnings\n", #warnings
    "warnings.filterwarnings('ignore')\n",
    "\n",
    "mic_data=pd.read_csv('Microsoft_Data.csv')\n",
    "\n",
    "mic_data\n",
    "\n",
    "missing_values=pd.DataFrame({'Column Name':mic_data.columns,'Missing Values Count':mic_data.isnull().sum(),\n",
    "                            'Missing values percentage':mic_data.isnull().sum()/mic_data.shape[0]*100})\n",
    "\n",
    "missing_values.sort_values(by='Missing values percentage',ascending=False)\n",
    "\n",
    "cols_to_drop=list(missing_values[missing_values['Missing values percentage']>50].index)\n",
    "cols_to_drop\n",
    "\n",
    "for col in mic_data.columns:\n",
    "    if mic_data[col].nunique()==mic_data.shape[0] or mic_data[col].nunique()==1:\n",
    "        cols_to_drop.append(col)\n",
    "\n",
    "mic_data.drop(columns=cols_to_drop,inplace=True)\n",
    "\n",
    "y=mic_data['HasDetections']\n",
    "mic_data.drop(columns='HasDetections',inplace=True)\n",
    "\n",
    "y.value_counts()\n",
    "\n",
    "cols_to_drop\n",
    "\n",
    "mic_data.shape"
   ]
  },
